    "import time\n",
    "start_time = time.time() \n",
    "def fib(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else:\n",
    "        return fib(n-1) + fib(n-2)\n",
    "print(fib(40))\n",
    "print(\"--- %s seconds ---\" % (time.time() - start_time))"
   ]
  }
